{
  "compilerOptions": {
    "baseUrl": "",
    "declaration": false,
    "experimentalDecorators": true,
    "lib": [
      "es2016",
      "dom"
    ],
    "mapRoot": "./",
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "../dist/out-tsc",
    "sourceMap": true,
    "target": "ES2022",
    "typeRoots": [
      "../node_modules/@types"
    ],
    "types": [
      "jasmine"
    ],
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "useDefineForClassFields": false
  },
  "files": [
    "main.ts",
    "polyfills.ts"
  ]
}
